# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'EatHealthy' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for EatHealthy

    # Pods for Firebase
    pod 'Firebase/Core'
    pod 'Firebase/Auth'
    pod 'Firebase/Storage'

    # Pods for HTTP
    pod 'Alamofire'
    pod 'SwiftyJSON'

    # Pods for Facebook
    pod 'Bolts'
    pod 'FBSDKCoreKit'
    pod 'FBSDKLoginKit'

    # Pods for UI
    pod 'SwiftMessages'

end
